	author = {Sebt, M. and Afshar, M. and Alipouri, Y.},
	month = nov,
	year = {2015},
	pages = {905--924},
}

@article{alcaraz_solving_2003,
	title = {Solving the {Multi}-{Mode} {Resource}-{Constrained} {Project} {Scheduling} {Problem} with genetic algorithms},
	volume = {54},
