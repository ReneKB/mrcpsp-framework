@article{hartmann_competitive_1998,
	title = {A competitive genetic algorithm for resource-constrained project scheduling},
	volume = {45},
	issn = {1520-6750},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/%28SICI%291520-6750%28199810%2945%3A7%3C733%3A%3AAID-NAV5%3E3.0.CO%3B2-C},
